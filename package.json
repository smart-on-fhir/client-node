{
  "name": "smart-client",
  "version": "1.1.1",
  "description": "SMART client for NodeJS",
  "main": "lib/index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "test": "lab -c -r console -o stdout -r html -o test/coverage.html -r lcov -o test/lcov.info -v",
    "test:watch": "nodemon -x 'npm test'",
    "build": "tsc --declaration",
    "build:watch": "tsc --watch",
    "start": "node examples/node",
    "start:express": "node examples/express",
    "start:hapi": "node examples/hapi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smart-on-fhir/client-node.git"
  },
  "keywords": [
    "smart",
    "fhir",
    "health",
    "hl7",
    "client"
  ],
