  "license": "APACHE2",
  "bugs": {
    "url": "https://github.com/smart-on-fhir/client-node/issues"
  },
  "homepage": "https://github.com/smart-on-fhir/client-node#readme",
  "dependencies": {
    "@types/jsonwebtoken": "^7.2.8",
    "axios": "^0.18.0",
    "jsonwebtoken": "^8.3.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/node": "^10.5.2",
    "code": "^5.2.0",
    "eslint": "^5.1.0",
    "lab": "^15.5.0",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
